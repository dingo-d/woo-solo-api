{
	"env": {
		"node": true
	},
	"extends": [
		"plugin:@wordpress/eslint-plugin/esnext"
	],
	"parser": "@babel/eslint-parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"rules": {
		"template-curly-spacing": 0,
		"space-in-parens": 0,
		"computed-property-spacing": 0
	}
}
